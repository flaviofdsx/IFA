      <xsd:simpleType name="QuantityType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Numeric integer information that is assigned or is determined by calculation,
          counting or sequencing. The value is integer without any decimal fraction. Format: n* with [n ... any digit | * ...
          unlimited repeats] (e.g. 5)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer"/>
      </xsd:simpleType>
      <xsd:simpleType name="NumericType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Numeric information that is assigned or is determined by calculation. The value does
          not require a unit of quantity or unit of measure but may have a decimal fraction. Format: n*[.n*] with [n ... any
          digit | * ... unlimited repeats]] (e.g. 27.56)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:decimal"/>
      </xsd:simpleType>
      <xsd:simpleType name="DateTimeType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A particular point in the progression of time together with the relevant
          supplementary information (ISO 8601). Format: [-]CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm] (e.g. '1998-05-15T12:37:00Z' for
          12:37 at the 15t of May 1998 according to UTC) with []: optional content | |: alternative content | (): sequence or
          group | CC: century (00 ... 99) | YY: year of a century (01 ... 99) | MM: month of a year (01 ... 12) | DD: day of a
          month (01 ... 31) | T: Starts the time part of a date | hh: hours of a day (00 ...23) | mm: minutes of a hour (00 ...
          59) | ss: seconds of a minute (00 ... 59) | Z: UTC indicator (does not distinguish between summer or winter time) |
          -: dates before crist (within the date part) / negative UTC offset (within the time zone part) locale offset differs
          between summer and winter time | +: positive UTC offset (within the time zone part) locale offset differs between
          summer and winter time</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:dateTime"/>
      </xsd:simpleType>
      <xsd:simpleType name="DateType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">One calendar day according the Gregorian calendar (ISO 8601). Format:
          [-]CCYY-MM-DD[Z|(+|-)hh:mm] (e.g. '1998-05-15' for the 15t of May 1998) with []: optional content | |: alternative
          content | (): sequence or group | CC: century (00 ... 99) | YY: year of a century (01 ... 99) | MM: month of a year
          (01 ... 12) | DD: day of a month (01 ... 31) | hh: hours of a day (00 ...23) | mm: minutes of a hour (00 ... 59) | -:
          dates before crist (within the date part) / negative UTC offset (within the time zone part) locale offset differs
          between summer and winter time | +: positive UTC offset (within the time zone part) locale offset differs between
          summer and winter time</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:date"/>
      </xsd:simpleType>
      <xsd:simpleType name="TimeType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The instance of time that occurs every day (ISO 8601). Format:
          hh:mm:ss[Z|(+|-)hh:mm] (e.g. '15:37:00+01:00' for 15:37 GMT) with (): sequence or group | |: alternative content | Z:
          UTC indicator (does not distinguish between summer or winter time) | -: negative UTC offset (locale offset differs
          between summer and winter time) | +: positive UTC offset (locale offset differs between summer and winter time) | hh:
          hours of a day (00 ...23) | mm: minutes of a hour (00 ... 59) | ss: seconds of a minute (00 ...
          59)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:time"/>
      </xsd:simpleType>
      <xsd:simpleType name="YearType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">One calendar year according the Gregorian calendar (ISO 8601). Format: CCYY (e.g.
          '2010') with CC: century (00 ... 99) | YY: year of a century (01 ... 99)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:gYear"/>
      </xsd:simpleType>
      <xsd:simpleType name="YearWeekType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Week within a year according the Gregorian calendar (ISO 8601). Format: CCYYWWW
          (e.g. '2007W22' for week 22 of 2007) with CC: century (00 ... 99) | YY: year of a century (01 ... 99) | W: Week
          indicator | WW: week of a year (01 ... 53)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:pattern value="[0-9]{4}[W][0-9]{2}">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">Unfortunately no xsd:gYearWeek does exist. This will do as a simple
              replacement.</xsd:documentation>
            </xsd:annotation>
          </xsd:pattern>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="IndicatorType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A list of two mutually exclusive Boolean values that express the only possible
          states of a property. Allowed values are 'true' or 'false'.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:boolean">
          <xsd:pattern value="false"/>
          <xsd:pattern value="true"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="LanguageTagCodeType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Multi-part string identifier for a language according to IETF RFC4646 (e.g. de,
          de-DE, de-Latn-DE, de-Latn-DE-1996). The Format of the tag is: langtag = (language["-" script]["-" region]*("-"
          variant)*("-" extension)["-" privateuse]).</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:pattern value="[a-zA-Z]{1,8}(-[a-zA-Z0-9]{1,8})*"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="CodeType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A character string that represents an element from a list of codes that is
          maintained by an external (e.g. ISO) or internal party (e.g. Integration Architecture). The code list is identified
          by a 'listID' attribute. The version of the code list (if any) is identified by the 'listVersionID' attribute. The
          textual equivalent for the code can be given with the 'name' attribute. In that case a 'languageID' attribute would
          state the language of the name.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:normalizedString">
            <xsd:attribute name="listID" type="xsd:normalizedString" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//ifoRef:Plant/@listID">
                      <iso:assert test="20 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:Plant/@listID [Error] Attribute length must be
                      lower or equal to 20!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">The identification of the code list used for value coding (e.g. NACE,
                ISO639-1, CL00123). 
                <ann:MaxLength>20</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="listVersionID" type="xsd:normalizedString" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//ifoRef:Plant/@listVersionID">
                      <iso:assert test="10 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:Plant/@listVersionID [Error] Attribute length must
                      be lower or equal to 10!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">The version of the code list if any (e.g. 1990 or 2). 
                <ann:MaxLength>10</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="name" type="xsd:normalizedString" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//ifoRef:Plant/@name">
                      <iso:assert test="100 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:Plant/@name [Error] Attribute length must be lower
                      or equal to 100!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">The textual equivalent of the code. This name may be used if the actual code
                value is not self explaining (e.g. the code value is 'V' and the name would be 'Volkswagen'). 
                <ann:MaxLength>100</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="languageID" type="tns:LanguageTagCodeType" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//ifoRef:Plant/@languageID">
                      <iso:assert test="50 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:Plant/@languageID [Error] Attribute length must be
                      lower or equal to 50!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">Specifies the language used for the 'name' attribute. Multi-part string
                identifier for a language according to IETF RCF4646 (e.g. de, de-DE, de-Latn-DE, de-Latn-DE-1996).
                <ann:MaxLength>50</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="LanguageCodeType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Character string identifying a language according to the encoding standards
          'ISO639-1' (e.g. de, en), 'ISA639-2' (e.g. deu, eng) or 'RCF4646' (e.g. de, de-DE, de-Latn-DE, de-Latn-DE-1996).
          'ISO639-1' is the default encoding. The encoding is stated by the 'listID' attribute. If the default encoding is used
          the 'listID' attribute can be ommited. 
          <ann:CodeList>RFC4646</ann:CodeList>
          <ann:CodeList>ISO639-1</ann:CodeList>
          <ann:CodeList>ISO639-2</ann:CodeList>
          </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:normalizedString">
            <xsd:attribute name="listID" type="xsd:normalizedString" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//tns:LanguageSelection/@listID">
                      <iso:assert test="20 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:LanguageSelection/@listID
                      [Error] Attribute length must be lower or equal to 20!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">The identification of encoding standard used for language coding (e.g.
                RFC4646, ISO639-1, ISO639-2). Default value is 'ISO639-1'. If this is used this attribute can be omitted. 
                <ann:MaxLength>20</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="listVersionID" type="xsd:normalizedString" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//tns:LanguageSelection/@listVersionID">
                      <iso:assert test="10 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:LanguageSelection/@listVersionID
                      [Error] Attribute length must be lower or equal to 10!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">The version of the encoding standard if any (e.g. 1990 or 2). 
                <ann:MaxLength>10</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="name" type="xsd:normalizedString" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//tns:LanguageSelection/@name">
                      <iso:assert test="100 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:LanguageSelection/@name
                      [Error] Attribute length must be lower or equal to 100!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">The textual equivalent of the code. This name may be used if the actual code
                value is not self explaining. (e.g. the code value is 'de' and the name would be 'german'). 
                <ann:MaxLength>100</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="languageID" type="tns:LanguageTagCodeType" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//tns:LanguageSelection/@languageID">
                      <iso:assert test="50 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:LanguageSelection/@languageID
                      [Error] Attribute length must be lower or equal to 50!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">Specifies the language used for the 'name' attribute. Multi-part string
                identifier for a language according to IETF RCF4646 (e.g. de, de-DE, de-Latn-DE, de-Latn-DE-1996).
                <ann:MaxLength>50</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="IdentifierType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A character string to identify and distinguish uniquely, one instance of an object
          in an identification scheme from all other objects in the same scheme together with relevant supplementary
          information. The identification scheme is stated by the 'schemeID', its version by the 'schemeVersionID'
          attribute.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="tns:String">
            <xsd:attribute name="schemeID" type="xsd:normalizedString" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//ifoRef:IFAOrderNumber/@schemeID">
                      <iso:assert test="50 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderNumber/@schemeID [Error] Attribute length
                      must be lower or equal to 50!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">The identification of the identification scheme (i.e. VIN) used to build the
                identifier. 
                <ann:MaxLength>50</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="schemeVersionID" type="xsd:normalizedString" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//ifoRef:IFAOrderNumber/@schemeVersionID">
                      <iso:assert test="10 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderNumber/@schemeVersionID [Error] Attribute
                      length must be lower or equal to 10!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">The version of the identification scheme (if any) used to define the
                identifier. 
                <ann:MaxLength>10</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="AmountType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A number of monetary units specified in a currency stated by the 'currency'
          attribute. Values allowed in the 'currency' attribute are determined by the code list ISO4217 (e.g.
          USD).</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:decimal">
            <xsd:attribute name="currency" type="xsd:normalizedString" use="required">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">Currency of the amount type value. The values are determined by the code list
                ISO4217 (e.g. USD). 
                <ann:CodeList>ISO4217</ann:CodeList>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="TextType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A character string (a finite set of characters) generally in the form of words of a
          particular language. The language of the text is determined by the 'languageID' attribute.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:string">
            <xsd:attribute name="languageID" type="tns:LanguageTagCodeType" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//ifoRef:CommissionNumber/@languageID">
                      <iso:assert test="50 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:CommissionNumber/@languageID [Error] Attribute
                      length must be lower or equal to 50!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">Multipart string identifier for a language acording to IETF RCF4646 (e.g. de,
                de-DE, de-Latn-DE, de-Latn-DE-1996).
                <ann:MaxLength>50</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="NameType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A character string that constitutes the distinctive designation of a person, place,
          thing or concept. The language of the name is determined by the 'languageID' attribute.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="tns:String">
            <xsd:attribute name="languageID" type="tns:LanguageTagCodeType" use="optional">
              <xsd:annotation>
                <xsd:appinfo>
                  <iso:pattern name="maxLength">
                    <iso:rule context="//com:Hostname/@languageID">
                      <iso:assert test="50 &gt;= string-length(.)">[Location]
                      //{http://xmldefs.volkswagenag.com/DD/Commons}:Hostname/@languageID [Error] Attribute length must be
                      lower or equal to 50!</iso:assert>
                    </iso:rule>
                  </iso:pattern>
                </xsd:appinfo>
                <xsd:documentation xml:lang="en">Multipart string identifier for a language acording to IETF RCF4646 (e.g. de,
                de-DE, de-Latn-DE, de-Latn-DE-1996).
                <ann:MaxLength>50</ann:MaxLength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
    </xsd:schema>
