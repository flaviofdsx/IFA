      <xsd:complexType name="DistributionReportType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="1" name="EntityVersion" type="bas:NumericType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifo:DistributionReport/ifo:EntityVersion">
                    <iso:assert test="3 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:DistributionReport/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:EntityVersion
                    [Error] Element length must be lower or equal to 3!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Version of the entity 
              <ann:MaxLength>3</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ModificationOperator" type="bas:CodeType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifo:DistributionReport/ifo:ModificationOperator">
                    <iso:assert test="1 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:DistributionReport/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:ModificationOperator
                    [Error] Element length must be lower or equal to 1!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Modification operator 
              <ann:CodeList>CL01369</ann:CodeList>
              <ann:MaxLength>1</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ReportStatus" type="bas:CodeType">
            <xsd:annotation>
              <xsd:appinfo/>
              <xsd:documentation>Report status which is sent in the distribution messages or generated during processing of the
              distribution messages.
              <ann:CodeList>CL01362</ann:CodeList>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ReportStatusTimestamp" type="bas:DateTimeType">
            <xsd:annotation>
              <xsd:documentation>Report status timestamp</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="TrackingPointNumber" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifo:DistributionReport/ifo:TrackingPointNumber">
                    <iso:assert test="4 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:DistributionReport/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:TrackingPointNumber
                    [Error] Element length must be lower or equal to 4!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Unique, numeric and unchangeable encoding of the checkpoint. 
              <ann:MaxLength>4</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="SerialNumber" type="bas:NumericType">
            <xsd:annotation>
              <xsd:documentation>Serial number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="Type" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifo:DistributionReport/ifo:Type">
                    <iso:assert test="1 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:DistributionReport/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:Type
                    [Error] Element length must be lower or equal to 1!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Type 
              <ann:MaxLength>1</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="OrderDate" type="bas:DateType">
            <xsd:annotation>
              <xsd:documentation>Order date</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="Customer" type="bas:TextType">
