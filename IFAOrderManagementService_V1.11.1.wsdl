              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifo:IndividualAddressEntity/ifo:PhoneNumber">
                    <iso:assert test="24 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:IndividualAddressEntity/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:PhoneNumber
                    [Error] Element length must be lower or equal to 24!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Phone number including the international country code (e. g.: +49122345657) 
              <ann:MaxLength>24</ann:MaxLength>
