      <xsd:complexType name="CommissionType">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="BID" type="bas:IdentifierType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifoRef:Commission/ifoRef:BID">
                    <iso:assert test="3 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:Commission/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:BID
                    [Error] Element length must be lower or equal to 3!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation xml:lang="en">Orderer ID (BestellerID) Usually this identifier is defined from KVPS the group
              system for importer and dealer data. e.g: 210 for Volkswagen Group UK. 
              <ann:MaxLength>3</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CommissionNumber" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifoRef:Commission/ifoRef:CommissionNumber">
                    <iso:assert test="6 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:Commission/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:CommissionNumber
                    [Error] Element length must be lower or equal to 6!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Commission number (6digit alpha). The seventh digit is fix 0 (zero) and will not be
              transmitted. 
              <ann:MaxLength>6</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CommissionNumberYear" type="bas:YearType">
            <xsd:annotation>
              <xsd:documentation>Year the commission number was issued.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ProductionNumberType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="Year" type="bas:YearType">
            <xsd:annotation>
              <xsd:documentation>"The year the vehicle is set to be built (Sollproduktionsjahr) (4-digit)"</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="Week" type="bas:YearWeekType">
            <xsd:annotation>
              <xsd:documentation>Week</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="Day" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifoRef:ProductionNumber/ifoRef:Day">
                    <iso:assert test="1 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:ProductionNumber/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:Day
                    [Error] Element length must be lower or equal to 1!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Production day of the week (comes from SONATA) (1-digit). 
              <ann:MaxLength>1</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="NumberID" type="bas:NumericType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifoRef:ProductionNumber/ifoRef:NumberID">
                    <iso:assert test="7 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:ProductionNumber/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:NumberID
                    [Error] Element length must be lower or equal to 7!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Sequence number generated per day (4-digit (Porsche: 7-digit)). 
              <ann:MaxLength>7</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="Plant" type="bas:CodeType">
            <xsd:annotation>
              <xsd:appinfo/>
              <xsd:documentation>Code of the plant where the vehicle shall be produced. Identifies the plant where the vehicle
              is built. This represents the CP8 plant (2-digit) (e.g. 11 for Wolfsburg). 
              <ann:CodeList>CL00359</ann:CodeList>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="CombinedProductionNumber" type="bas:String">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifoRef:ProductionNumber/ifoRef:CombinedProductionNumber">
                    <iso:assert test="255 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:ProductionNumber/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:CombinedProductionNumber
                    [Error] Element length must be lower or equal to 255!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Combined production number. Note: this element will be replaced by seperate elements in a
              later versions. If seperate elements are alredy used now, the leading zero might need to be added.
              <ann:MaxLength>255</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ForeignSystemKeyType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="1" name="Type" type="bas:CodeType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifoRef:ForeignKey/ifoRef:Type">
                    <iso:assert test="10 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:ForeignKey/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:Type
                    [Error] Element length must be lower or equal to 10!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Type of the key.(Examples: PORSCHE, COPS) 
              <ann:CodeList>CL01372</ann:CodeList>
              <ann:MaxLength>10</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="1" name="Key" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifoRef:ForeignKey/ifoRef:Key">
                    <iso:assert test="30 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:ForeignKey/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:Key
                    [Error] Element length must be lower or equal to 30!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>The key itself. 
              <ann:MaxLength>30</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
