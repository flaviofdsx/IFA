              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifo:IndividualAddressEntity/ifo:Contact">
                    <iso:assert test="60 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:IndividualAddressEntity/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:Contact
                    [Error] Element length must be lower or equal to 60!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>name of contact partner 
              <ann:MaxLength>60</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PhoneNumber" type="bas:TextType">
