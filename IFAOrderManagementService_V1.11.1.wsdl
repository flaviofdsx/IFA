            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifo:RegistrationDocument/ifo:RegistrationDocumentNumber">
                    <iso:assert test="10 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:RegistrationDocument/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:RegistrationDocumentNumber
