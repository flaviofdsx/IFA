      <xsd:element name="IFAOrderRef" type="tns:IFAOrderIdentifierType"/>
      <xsd:complexType name="IFAOrderIdentifierType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Identifies an IFAOrder uniquely.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="IFAOrderNumber" type="bas:IdentifierType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifoRef:IFAOrderRef/ifoRef:IFAOrderNumber">
                    <iso:assert test="13 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderRef/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderNumber
                    [Error] Element length must be lower or equal to 13!</iso:assert>
                  </iso:rule>
                </iso:pattern>
                <iso:pattern name="exclude">
                  <iso:rule context="/tns:CreateIFAOrder/ifo:IFAOrder/ifo:IFAOrderIdentifier">
                    <iso:assert test="not(ifoRef:IFAOrderNumber)">[Location]
                    /{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:CreateIFAOrder/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:IFAOrder/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:IFAOrderIdentifier/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderNumber
                    [Error] Element 'IFAOrderNumber' is not allowed!</iso:assert>
                  </iso:rule>
                </iso:pattern>
                <iso:pattern name="exclude">
                  <iso:rule context="/tns:CreateIFAOrder/ifo:IFAOrder/ifo:KeyStatusEntity/ifoRef:IFAOrderRef">
                    <iso:assert test="not(ifoRef:IFAOrderNumber)">[Location]
                    /{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:CreateIFAOrder/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:IFAOrder/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:KeyStatusEntity/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderRef/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderNumber
                    [Error] Element 'IFAOrderNumber' is not allowed!</iso:assert>
                  </iso:rule>
                </iso:pattern>
                <iso:pattern name="exclude">
                  <iso:rule context="/tns:ChangeIFAOrder/ifo:IFAOrder/ifo:KeyStatusEntity/ifoRef:IFAOrderRef">
                    <iso:assert test="not(ifoRef:IFAOrderNumber)">[Location]
                    /{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:ChangeIFAOrder/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:IFAOrder/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:KeyStatusEntity/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderRef/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderNumber
                    [Error] Element 'IFAOrderNumber' is not allowed!</iso:assert>
                  </iso:rule>
                </iso:pattern>
                <iso:pattern name="exclude">
                  <iso:rule context="/tns:GetPriceSimulation/ifo:IFAOrder/ifo:KeyStatusEntity/ifoRef:IFAOrderRef">
                    <iso:assert test="not(ifoRef:IFAOrderNumber)">[Location]
                    /{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceSimulation/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:IFAOrder/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:KeyStatusEntity/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderRef/{http://xmldefs.volkswagenag.com/DD/IFAOrder/Ref/V1}:IFAOrderNumber
                    [Error] Element 'IFAOrderNumber' is not allowed!</iso:assert>
