              <xsd:documentation>The currency of the price 
              <ann:MaxLength>3</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PriceResponsible" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:PriceResponsible">
                    <iso:assert test="2 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:PriceResponsible
                    [Error] Element length must be lower or equal to 2!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Indication who is reponsible for the price. 
              <ann:MaxLength>2</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="TotalPrice" type="bas:AmountType">
            <xsd:annotation>
              <xsd:documentation>The total price</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ReturnTotal" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:ReturnTotal">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:ReturnTotal
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Return-Code depending on TotalPrice
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="CountrySalesProgram" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:CountrySalesProgram">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:CountrySalesProgram
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Country basis for the itemization of a vehicle
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="CountrySalesProgramVersion" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:CountrySalesProgramVersion">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:CountrySalesProgramVersion
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Version of country sales program
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="CountrySalesProgramPrice" type="bas:AmountType">
            <xsd:annotation>
              <xsd:documentation>Part of the TotalPrice</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="CountrySalesProgramPriceReturn" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:CountrySalesProgramPriceReturn">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:CountrySalesProgramPriceReturn
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Return code depending on CountrySalesProgram.
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PricingBasePrice" type="bas:AmountType">
            <xsd:annotation>
              <xsd:documentation>Part of the total price.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PricingBasePriceGroup" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:PricingBasePriceGroup">
                    <iso:assert test="3 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:PricingBasePriceGroup
                    [Error] Element length must be lower or equal to 3!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Version of country sales program. 
              <ann:MaxLength>3</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PricingBasePriceGroupReturn" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:PricingBasePriceGroupReturn">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:PricingBasePriceGroupReturn
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Return code depending on PricingBasePrice.
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="CountrySalesProgramPNRPrice" type="bas:AmountType">
            <xsd:annotation>
              <xsd:documentation>Part of the TotalPrice</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="CountrySalesProgramPNRPriceReturn" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:CountrySalesProgramPNRPriceReturn">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:CountrySalesProgramPNRPriceReturn
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Return code depending on the CountrySalesProgramPNRPrice
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ModelCoding" type="bas:CodeType">
            <xsd:annotation>
              <xsd:appinfo/>
              <xsd:documentation>Model code. Possible values are limited by the following definitions: CL00027 (MBV): selling
              code of the VW conglomerate (e.g. 1J1RZ7 according to the MBV system); 
              <ann:CodeList>CL00027</ann:CodeList>CL00236 (ATLAS): european type-approval number (e.g. e1*2001/116*0160*21); 
              <ann:CodeList>CL00236</ann:CodeList>CL00310 (Maneck): Volkswagen Group internal reference code to model
              assignment based on the Maneck list; 
              <ann:CodeList>CL00310</ann:CodeList>CL00442 (KUBA): Coding defined in KUBA 
              <ann:CodeList>CL00442</ann:CodeList>CL00356 (KBA): model key (TSN) defined by German Federal Motor Vehicle
              Authority (Kraftfahrtbundesamt), which is only unique with the manufacturer key defined by KBA; 
              <ann:CodeList>CL00356</ann:CodeList>DAT: vehicle main type according to the DAT organisation; CL01102 (MBT):
              selling code of the VW conglomerate according to the MBT system; 
              <ann:CodeList>CL01102</ann:CodeList>Porsche has its own ModelCodes 
              <ann:CodeList>CL01343</ann:CodeList>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ModelPrice" type="bas:AmountType">
            <xsd:annotation>
              <xsd:documentation>Price of the model.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ModelPriceGroup" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:ModelPriceGroup">
                    <iso:assert test="3 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:ModelPriceGroup
                    [Error] Element length must be lower or equal to 3!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Price group of the model code. 
              <ann:MaxLength>3</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ModelReturn" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:ModelReturn">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:ModelReturn
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Return code depending on PricingBasePrice
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ColourCode" type="bas:CodeType">
            <xsd:annotation>
              <xsd:appinfo/>
              <xsd:documentation>Interior color, possible values are RAL, VW internal and other codes 
              <ann:CodeList>CL00028</ann:CodeList>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ColourPrice" type="bas:AmountType">
            <xsd:annotation>
              <xsd:documentation>Price of the color.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ColourPriceGroup" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:ColourPriceGroup">
                    <iso:assert test="3 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:ColourPriceGroup
                    [Error] Element length must be lower or equal to 3!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Price group of the color. 
              <ann:MaxLength>3</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ColourReturn" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:ColourReturn">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:ColourReturn
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Return code depending on ColourPrice.
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PRNumbersTotal" type="bas:NumericType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceResponse/tns:PRNumbersTotal">
                    <iso:assert test="3 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceResponse/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:PRNumbersTotal
                    [Error] Element length must be lower or equal to 3!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Total number of PR numbers. 
              <ann:MaxLength>3</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PRNumbers" type="tns:PRNumbersReturnType">
            <xsd:annotation>
              <xsd:documentation>List of PR numbers</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="Errors" type="tns:ErrorsType">
            <xsd:annotation>
              <xsd:documentation>A list of errors.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PRNumbersReturnType">
        <xsd:sequence>
          <xsd:element maxOccurs="800" minOccurs="1" name="PRNumber" type="tns:PRNumberReturnType">
            <xsd:annotation>
              <xsd:documentation>Up to 800 PR numbers</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PRNumberReturnType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="PRNumber" type="bas:CodeType">
            <xsd:annotation>
              <xsd:appinfo/>
              <xsd:documentation>The PR number itself 
              <ann:CodeList>CL00364</ann:CodeList>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="FinancialStatus" type="bas:CodeType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:PRNumber/tns:FinancialStatus">
                    <iso:assert test="1 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:PRNumber/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:FinancialStatus
                    [Error] Element length must be lower or equal to 1!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Finance-related status of PR numbers: E (External) I (Internal) L (Standard PR number) M
              (Minimum setting) K (Zero price) G (Packages) P (Package content) 
              <ann:CodeList>CL01352</ann:CodeList>
              <ann:MaxLength>1</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PRNumberPrice" type="bas:AmountType">
            <xsd:annotation>
              <xsd:documentation>Price for the specific PRNumber.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PRNumberPriceGroup" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:PRNumber/tns:PRNumberPriceGroup">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:PRNumber/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:PRNumberPriceGroup
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>PriceGroup for the specific PRNumber.
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PRNumberReturn" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:PRNumber/tns:PRNumberReturn">
                    <iso:assert test="4096 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:PRNumber/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:PRNumberReturn
                    [Error] Element length must be lower or equal to 4096!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Return code depending on PRNumberPrice.
              <ann:MaxLength>4096</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="Descriptions" type="tns:PRNumbersDescriptionListType">
            <xsd:annotation>
              <xsd:documentation>List of up to three text lines</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PRNumbersDescriptionListType">
        <xsd:sequence>
          <xsd:element maxOccurs="3" minOccurs="0" name="Description" type="tns:PRNumberDescriptionElementType">
            <xsd:annotation>
              <xsd:documentation>One description element within the list</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PRNumberDescriptionElementType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="Text" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:Description/tns:Text">
                    <iso:assert test="100 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:Description/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:Text
                    [Error] Element length must be lower or equal to 100!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>The text within the description element. 
              <ann:MaxLength>100</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="GetPriceSimulationType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="1" ref="ifo:IFAOrder">
            <xsd:annotation>
              <xsd:documentation>Including the business object IFAOrder</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="RatingDate" type="bas:DateType">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">Date at which/for which the price was rated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="LanguageSelection" type="bas:LanguageCodeType">
            <xsd:annotation>
              <xsd:appinfo/>
              <xsd:documentation>Selection of the language for errors in the response 
              <ann:CodeList>RFC4646</ann:CodeList>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="ErrorLayoutFlag" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:GetPriceSimulation/tns:ErrorLayoutFlag">
                    <iso:assert test="1 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:GetPriceSimulation/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:ErrorLayoutFlag
                    [Error] Element length must be lower or equal to 1!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>Flag indicating which error layout shall be sent in the response message. 
              <ann:MaxLength>1</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="TextSelectionFlag" type="bas:IndicatorType">
            <xsd:annotation>
              <xsd:documentation>Flag for selecting that texts shall be inluded in the response. False is
              default.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="GetPriceSimulationResponseType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="GetPriceResponse" type="tns:GetPriceResponseType">
            <xsd:annotation>
              <xsd:documentation>Type for the collection of price-related information</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ErrorsType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="ErrorsTotal" type="bas:NumericType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//tns:Errors/tns:ErrorsTotal">
                    <iso:assert test="4 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:Errors/{http://xmldefs.volkswagenag.com/COP/VehicleManufacturing/IFAOrderManagementService/V1}:ErrorsTotal
                    [Error] Element length must be lower or equal to 4!</iso:assert>
                  </iso:rule>
                </iso:pattern>
              </xsd:appinfo>
              <xsd:documentation>The total number of errors. 
              <ann:MaxLength>4</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="Errors" type="tns:ErrorListType">
            <xsd:annotation>
              <xsd:documentation>List of errors.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ErrorListType">
        <xsd:sequence>
          <xsd:element maxOccurs="2000" minOccurs="0" name="Error" type="tns:ErrorType">
            <xsd:annotation>
              <xsd:documentation>One error within the list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ErrorType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="1" name="ErrorCode" type="bas:TextType">
            <xsd:annotation>
