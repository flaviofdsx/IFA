              <xsd:documentation>Fax number including the international country code (e. g.: +49122345657) 
              <ann:MaxLength>24</ann:MaxLength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="MailAddress" type="bas:TextType">
            <xsd:annotation>
              <xsd:appinfo>
                <iso:pattern name="maxLength">
                  <iso:rule context="//ifo:IndividualAddressEntity/ifo:MailAddress">
                    <iso:assert test="64 &gt;= string-length(text())">[Location]
                    //{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:IndividualAddressEntity/{http://xmldefs.volkswagenag.com/DD/IFAOrder/V1}:MailAddress
                    [Error] Element length must be lower or equal to 64!</iso:assert>
